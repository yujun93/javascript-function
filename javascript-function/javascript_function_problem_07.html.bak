		var test;
		var str=0;
	
		for(var i=0;arguments.length;i++){
			for(var j=0;arguments[i][j].length;j++){
				for(var z=0;arguments[i].length;z++){
					if(arguments[i][z].length < j){                  //32~47 다른방법으로 str에 3차원 배열 원소값을 다더한후 test 문자열에 저장 반복문 끝난후 test값 리턴 (38~40 오류 미해결..)
					str += arguments[i][z][j];		
					}	
				}
				test += str.toString() + " ";
				str=0;
			}
			test += "\n";
		}
		return test;
      }

